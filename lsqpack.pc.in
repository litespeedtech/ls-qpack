URL: @CMAKE_PROJECT_HOMEPAGE_URL@
Version: @PROJECT_VERSION@
Requires: @LSQPACK_DEPENDS@
Cflags: -I"${includedir}"
Libs: -L"${libdir}" -llsqpack
